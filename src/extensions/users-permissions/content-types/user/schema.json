{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "fullname": {
      "type": "string",
      "required": false
    },
    "birth": {
      "type": "date"
    },
    "phone": {
      "type": "string",
      "minLength": 10,
      "maxLength": 11,
      "required": true,
      "regex": "^[1-9][0-9]{9,10}$"
    },
    "loyaltyPoints": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "totalSpent": {
      "type": "decimal",
      "default": 0,
      "min": 0
    },
    "totalOrders": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "preferences": {
      "type": "json"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "user"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "user"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "user"
    },
    "chatsSent": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat.chat",
      "mappedBy": "sender"
    },
    "chatsReceived": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat.chat",
      "mappedBy": "receiver"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "user"
    },
    "cpf": {
      "type": "string",
      "maxLength": 11,
      "required": false,
      "unique": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ]
    },
    "current_role": {
      "type": "enumeration",
      "default": "customer",
      "enum": [
        "customer",
        "driver"
      ]
    },
    "tos_customer": {
      "type": "boolean",
      "required": true
    },
    "tos_customer_date": {
      "type": "datetime",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "tos_customer"
            },
            true
          ]
        }
      },
      "required": true
    },
    "tos_driver": {
      "type": "boolean"
    },
    "tos_driver_date": {
      "type": "datetime",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "tos_driver"
            },
            true
          ]
        }
      }
    },
    "referrer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
