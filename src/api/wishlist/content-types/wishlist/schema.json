{
  "kind": "collectionType",
  "collectionName": "wishlists",
  "info": {
    "singularName": "wishlist",
    "pluralName": "wishlists",
    "displayName": "Wishlist",
    "description": "Lista de desejos dos clientes"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer",
      "inversedBy": "wishlists"
    },
    "name": {
      "type": "string",
      "required": true,
      "default": "Minha Lista"
    },
    "description": {
      "type": "text"
    },
    "isDefault": {
      "type": "boolean",
      "default": true
    },
    "isPublic": {
      "type": "boolean",
      "default": false
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "wishlists"
    },
    "combos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::combo.combo",
      "mappedBy": "wishlists"
    },
    "totalItems": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "lastActivity": {
      "type": "datetime"
    },
    "metadata": {
      "type": "json",
      "default": {}
    }
  }
}
